# Queue Provider Configuration
# Options: SQS, RABBITMQ
QUEUE_PROVIDER=SQS

# Application Configuration
PORT=3000
NODE_ENV=development

# AWS SQS Configuration (for SQS provider)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_ENDPOINT=http://localhost:4566
SQS_QUEUE_URL=http://localhost:4566/000000000000/space-messages

# RabbitMQ Configuration (for RABBITMQ provider)
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_QUEUE=space-messages
